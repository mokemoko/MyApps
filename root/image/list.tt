[% META
  title = "一覧"
%]
[% WRAPPER wrapper.tt %]
  [% base = "/static/images/" %]
  [% FOREACH image = c.stash.images %]
    <span id="[% image.gid %]" class="controllable" style="position:relative">
    <a href="[% c.config.image_domain _ '/post/show/' _ image.gid %]">
      <img src="[% base _ 'thumb/' _ image.gid _ '.jpg' %]" />
    </a>
    </span>
  [% END %]
[% END %]
